				android:centerColor="@color/lt_green"
				android:endColor="@color/dk_green"
				android:type="linear" />
			 
			<padding
				android:bottom="@dimen/groupPadding"
				android:left="@dimen/groupPadding"
				android:right="@dimen/groupPadding"
